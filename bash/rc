export UPSHELL=1

[ "$PS1" ] || return

[ "$BASH" ] && UPSHELL_PHASE=pre-interactive.bash . ~/.upshell/bin/dispatch
UPSHELL_PHASE=pre-interactive.sh . ~/.upshell/bin/dispatch
[ "$BASH" ] && UPSHELL_PHASE=interactive.bash . ~/.upshell/bin/dispatch
UPSHELL_PHASE=interactive.sh . ~/.upshell/bin/dispatch

if [ -z "$UPSHELL_NO_ETC_BASHRC" ] && [ -r /etc/bashrc ] ; then
   . /etc/bashrc
fi

UPSHELL_PHASE=post-interactive.sh . ~/.upshell/bin/dispatch
[ "$BASH" ] && UPSHELL_PHASE=post-interactive.bash . ~/.upshell/bin/dispatch
